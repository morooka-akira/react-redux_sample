{
    "env": {
        "browser": true,
        "jest": true
    },
    "extends": [
        "standard",
        "plugin:flowtype/recommended",
        "plugin:react/recommended",
        "plugin:react-redux/recommended"
    ],
    "plugins" : [
        "compat",
        "flowtype",
        "react",
        "react-redux"
    ],
    "rules": {
        "react/prop-types": 0,
        "no-duplicate-imports": 0
    },
    "settings": {
        "polifills": ["fetch"]
    }
}
